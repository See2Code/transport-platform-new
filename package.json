{
  "name": "transport-platform",
  "version": "1.6.4",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/inter": "^5.2.5",
    "@google-cloud/firestore": "^7.11.0",
    "@google-cloud/storage": "^7.16.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.15.14",
    "@mui/system": "^7.0.2",
    "@mui/x-data-grid": "^7.28.3",
    "@mui/x-date-pickers": "^6.11.2",
    "@react-google-maps/api": "^2.20.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.8",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/recharts": "^1.8.29",
    "date-fns": "^2.30.0",
    "firebase": "^10.14.1",
    "firebase-tools": "^14.1.0",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "leaflet": "^1.9.4",
    "notistack": "^3.0.1",
    "react": "18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "18.2.0",
    "react-i18next": "^15.5.1",
    "react-image-crop": "^10.1.8",
    "react-leaflet": "^4.2.1",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.1",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check-unused": "ts-unused-exports tsconfig.json",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:detail": "NODE_ENV=production GENERATE_SOURCEMAP=true npx webpack-bundle-analyzer build/static/js/main.*.js",
    "build:analyze": "GENERATE_SOURCEMAP=true craco build",
    "lint": "eslint src --ext .ts,.tsx --fix --rule '@typescript-eslint/no-unused-vars: [warn, {argsIgnorePattern: ^_, varsIgnorePattern: ^_}]' --rule 'unused-imports/no-unused-vars: [warn, {argsIgnorePattern: ^_, varsIgnorePattern: ^_}]'",
    "lint-clean": "eslint src --ext .ts,.tsx --fix --rule '@typescript-eslint/no-unused-vars: [warn, {argsIgnorePattern: ^_, varsIgnorePattern: ^_}]' --rule 'unused-imports/no-unused-vars: [warn, {argsIgnorePattern: ^_, varsIgnorePattern: ^_}]'",
    "lint-strict": "eslint src --ext .ts,.tsx --fix",
    "lint-ignore-hooks": "eslint src --ext .ts,.tsx --fix --rule 'react-hooks/exhaustive-deps: off'",
    "fix-unused-vars": "node fix-unused-vars.js",
    "fix-theme-args": "node fix-theme-args.js",
    "fix-all": "npm run fix-unused-vars && npm run fix-theme-args && npm run lint-ignore-hooks"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "firebase-admin": "^13.2.0",
    "framer-motion": "^12.6.2",
    "lodash": "^4.17.21",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.8.3"
  }
}
